{
  "name": "wheel-of-tbr-server",
  "version": "1.0.0",
  "description": ">Spin the wheel and receive a prompt.<br /> >Gamify your reading experience.<br /> >Challenge yourself to read more.<br />",
  "main": "index.js",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.18",
